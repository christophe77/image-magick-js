    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": "12.x || 14.x || >= 16"
  },
